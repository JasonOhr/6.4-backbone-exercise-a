<div class="multi-container">
    <div class="add-blog"><span><a href="#e/create">+</a></span></div>

    {{#each this}}
    <div class="blog-single" id="{{id}}">
        <span class="delete-blog">X</span>
        <a href="#e/{{id}}">
            <div class="blog-title"><h1>{{title}}</h1></div>
            <div class="blog-content"><p>{{content}}</p></div>
        </a>
    </div>

    {{/each}}

</div>

